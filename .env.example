# Frigate Environment Configuration
# Copy this file to .env and customize the values

# Timezone - Change to your timezone
# See: https://en.wikipedia.org/wiki/List_of_tz_database_time_zones
TIMEZONE=America/New_York

# RTSP Credentials (used for restreaming)
FRIGATE_RTSP_USER=admin
FRIGATE_RTSP_PASSWORD=your_secure_password_here

# MQTT Configuration (optional - for Home Assistant integration)
# Leave empty if not using MQTT
MQTT_HOST=
MQTT_PORT=1883
MQTT_USER=
MQTT_PASSWORD=
